        public Skill(MudLocalizer localizer)
        {
            _localizer = localizer;
        }

        public void SetUpSelectedSkill(string skillName)
        {
            Icon = _allSkillIcons[skillName];
            switch (skillName)
            {
                case ".NET Core":
                    SkillDescription = _localizer[".NetCoreDesc"];
